config_version=5

[application]

config/name="Platformer"
config/tags=PackedStringArray("inprogress")
run/main_scene="res://Scenes/Menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://Art/Icon/GameIcon.png"

[audio]

buses/default_bus_layout="res://AudioBus/default_bus_layout.tres"

[autoload]

SettingsMenu="*res://Scenes/Menu/settings_menu.tscn"
SceneManager="*res://Scenes/Menu/scene_manager.tscn"
GameManager="*res://Script/System/game_manager.gd"
TimeManager="*res://Script/System/time_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Platformer"

[editor_plugins]

enabled=PackedStringArray("res://addons/cinematic_camera_2d/plugin.cfg")

